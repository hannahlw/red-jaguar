<h1><%= @board.name %></h1>
<h3>Views: <%= @board.views %></h3>
<h4><% @board.tags.each do |tag| %>
<li>Tags: <%= tag.name  %></li>
<% end %>
</h4>

<h5>By <%= @board.user.name %></h5>


<% @twitter_links.each do |link| %>
 <div class="col-sm-4">
  <div class="card">
    <div class="card-header ch-alt">
      <h2>Margin <small>We can put text here</small></h2>                     
        </div>
            <iframe border=0 frameborder=0 height=250 width=450 
               src="http://twitframe.com/show?url=<%= link.url %>"></iframe>
    </div>
  </div>
<% end %>



<% @youtube_links.each do |link| %>
<div class="col-sm-4">                     
  <div class="card">
    <div class="card-header ch-alt">
      <h2>Margin <small>We can put text here</small></h2>
        </div>
<iframe width="450" height="250" src="<%= link.url.gsub(/watch\?v=/,"embed/") %>" frameborder="0" allowfullscreen></iframe>
</div>
</div>
<% end %>
</div>




<% @web_links.each do |link| %>
<div class="col-sm-4">                     
  <div class="card">
    <div class="card-header ch-alt">
      <h2>Margin <small>We can put text here</small></h2>
        </div>
<iframe width="450" height="250" src="<%= link.url %>" frameborder="0" allowfullscreen></iframe>
</div>
</div>
<% end %>
</div>

